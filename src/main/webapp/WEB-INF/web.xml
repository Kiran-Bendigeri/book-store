<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
  
  <welcome-file>welcome.html</welcome-file>
  
  </welcome-file-list>
  
  <servlet>  
   <servlet-name>InsertBook</servlet-name>  
   <servlet-class>com.js.controller.InsertBook</servlet-class>  
   <load-on-startup>0</load-on-startup>  
  </servlet>
  
  <servlet>  
   <servlet-name>GetBook</servlet-name>  
   <servlet-class>com.js.controller.GetBook</servlet-class>  
   <load-on-startup>0</load-on-startup>  
  </servlet>
  
  <servlet>  
   <servlet-name>DeleteBook</servlet-name>  
   <servlet-class>com.js.controller.DeleteBook</servlet-class>  
   <load-on-startup>0</load-on-startup>  
  </servlet>
  
</web-app>
